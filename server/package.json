{
  "name": "at-mt",
  "version": "1.0.0",
  "description": "backend for at-mt",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production node index.js",
    "dev": "NODE_ENV=development nodemon index.js",
    "deploy": "npm test && git push heroku master",
    "build:frontend": "rm -rf build && cd ../app && npm run build && cp -r build ../server",
    "deploy:full": "npm run build:frontend && git add . && git commit -m frontendbuild && git push && npm run deploy",
    "test": "NODE_ENV=test jest --verbose -runInBand",
    "logs:prod": "heroku logs --tail"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.9.12"
  },
  "devDependencies": {
    "jest": "^26.0.1",
    "nodemon": "^2.0.3",
    "supertest": "^4.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
